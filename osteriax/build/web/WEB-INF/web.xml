<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>com.controller.signup.signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>accept</servlet-name>
        <servlet-class>com.controller.accepting.accept</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminSignup</servlet-name>
        <servlet-class>com.controller.signup.adminSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAdmin</servlet-name>
        <servlet-class>com.controller.deleting.deleteAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteClinics</servlet-name>
        <servlet-class>com.controller.deleting.deleteClinics</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>comment</servlet-name>
        <servlet-class>com.controller.commenting.comment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.controller.login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reply</servlet-name>
        <servlet-class>com.controller.creply.reply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>accept</servlet-name>
        <url-pattern>/accept</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminSignup</servlet-name>
        <url-pattern>/adminSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAdmin</servlet-name>
        <url-pattern>/deleteAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteClinics</servlet-name>
        <url-pattern>/deleteClinics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>comment</servlet-name>
        <url-pattern>/comment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reply</servlet-name>
        <url-pattern>/reply</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
